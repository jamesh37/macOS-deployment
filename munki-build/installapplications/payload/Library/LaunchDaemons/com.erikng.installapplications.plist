<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.erikng.installapplications</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/python</string>
		<string>/Library/Application Support/installapplications/installapplications.py</string>
		<string>--jsonurl</string>
		<string>~URL~/demo.json</string>
		<string>--depnotify</string>
		<string>Command: WindowTitle: "Induction Healthcare Setup"</string>
		<string>Command: Image: /tmp/logo.png</string>
		<string>Command: MainTitle: Welcome to Induction Healthcare</string>
		<string>Command: MainText: Welcome to your new Induction Healthcare MacBook machine. We've got a few more steps to get your machine ready for first use. Hang tight and watch the screen.</string>
		<string>Status: Configuring Machine...</string>
		<string>DEPNotifyPath: /Applications/Utilities/DEPNotify.app</string>
		<string>DEPNotifyArguments: -munki</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/var/log/installapplications.log</string>
	<key>StandardErrorPath</key>
	<string>/var/log/installapplications.log</string>
</dict>
</plist>
